{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Supacortex",
  "version": "0.1.6",
  "identifier": "ai.supacortex.app",
  "build": {
    "frontendDist": "https://supacortex.ai/app",
    "devUrl": "http://localhost:3000/app"
  },
  "app": {
    "macOSPrivateApi": true,
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Supacortex",
        "width": 1280,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "transparent": true,
        "decorations": true,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "devtools": true
      }
    ],
    "security": {
      "csp": "default-src 'self' https://*.supacortex.ai https://supacortex.ai; connect-src 'self' https://*.supacortex.ai https://supacortex.ai http://127.0.0.1:* http://localhost:* ipc: tauri:; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://*.supacortex.ai https://supacortex.ai; style-src 'self' 'unsafe-inline' https://*.supacortex.ai https://supacortex.ai; img-src 'self' data: blob: https: http:; font-src 'self' data: https://*.supacortex.ai https://supacortex.ai"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDhGRDJBMUQyMjlGRDdCMEUKUldRT2UvMHAwcUhTaitscWFjLzJGeS9STUpIZnZLQ2JQalIweVllN3cvbkw0Njg1WDdvRkloM08K",
      "endpoints": [
        "https://github.com/monorepo-labs/supacortex/releases/latest/download/latest.json"
      ]
    }
  }
}
